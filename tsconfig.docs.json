{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    /* Linting */
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedSideEffectImports": true,
  },
  "include": [
    "docs/examples/**/*.ts",
    "docs/examples/**/*.tsx",
    "docs/examples/**/*.vue",
  ]
}
